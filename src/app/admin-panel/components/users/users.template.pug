section.users(tjsMaterialize)
  ag-grid-angular.ag-material.data-grid(
    '[rowData]'='users'
    '[gridOptions]'='gridOptions'
    '(gridReady)'='onGridReady($event)'
    '(rowClicked)'='onRowClicked($event)'
  )
  ngb-pagination.center.d-flex.justify-content-center(
    '[collectionSize]'='users?.length'
    '[(page)]'="page"
    '(pageChange)'='pageChanged(page)'
  )
